{
  "name": "rollup-plugin-bundle-analyzer",
  "version": "1.6.6",
  "publishConfig": {
    "access": "public"
  },
  "description": "rollup plugin that represents bundle content as convenient interactive zoomable treemap",
  "license": "MIT",
  "author": "xiaojie",
  "homepage": "",
  "bugs": "",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "exports": {
    "import": "./dist/es/index.js",
    "types": "./types/index.d.ts",
    "default": "./dist/cjs/index.js"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-decorators": "7.14.2",
    "@babel/plugin-transform-runtime": "7.14.3",
    "@babel/preset-env": "7.14.2",
    "@babel/preset-react": "7.13.13",
    "@babel/runtime": "7.14.0",
    "@carrotsearch/foamtree": "3.5.0",
    "@rollup/plugin-commonjs": "^23.0.0",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/lodash": "^4.14.186",
    "@types/opener": "^1.4.0",
    "@types/uglify-js": "^3.17.1",
    "@types/ws": "^8.5.3",
    "autoprefixer": "10.2.5",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "babel-plugin-lodash": "3.3.4",
    "chai": "4.3.4",
    "chai-subset": "1.6.0",
    "classnames": "2.3.1",
    "core-js": "3.12.1",
    "css-loader": "5.2.5",
    "cssnano": "5.0.4",
    "del": "6.0.0",
    "del-cli": "^5.0.0",
    "eslint-plugin-react": "7.23.2",
    "filesize": "^6.3.0",
    "globby": "11.0.3",
    "jest": "27.2.2",
    "mobx": "5.15.7",
    "mobx-react": "6.3.1",
    "postcss": "8.3.0",
    "postcss-icss-values": "2.0.2",
    "postcss-loader": "5.3.0",
    "preact": "10.5.13",
    "rollup": "^3.0.0-7",
    "stream-combiner2": "1.1.1",
    "style-loader": "2.0.0",
    "terser-webpack-plugin": "5.1.2",
    "typescript": "^4.8.3",
    "url-loader": "4.1.1",
    "webpack": "5.37.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.2"
  },
  "repository": "https://github.com/Jerry2023/rollup-plugin-bundle-analyzer",
  "scripts": {
    "build:client": "webpack",
    "build-all": "pnpm run build:client & pnpm run build",
    "build": "rollup -c",
    "ci:coverage": "nyc pnpm test && nyc report --reporter=text-lcov > coverage.lcov",
    "ci:lint": "pnpm build && pnpm lint",
    "ci:lint:commits": "commitlint --from=${CIRCLE_BRANCH} --to=${CIRCLE_SHA1}",
    "ci:test": "pnpm test -- --verbose",
    "prebuild": "del-cli dist",
    "prepare": "if [ ! -d 'dist' ]; then pnpm build; fi",
    "prerelease": "pnpm build",
    "pretest": "pnpm build",
    "release": "pnpm --workspace-root plugin:release --pkg $npm_package_name",
    "test": "ava",
    "test:ts": "tsc --noEmit"
  },
  "files": [
    "dist",
    "!dist/**/*.map",
    "types",
    "README.md",
    "public",
    ""
  ],
  "keywords": [
    "vite",
    "rollup",
    "plugin",
    "resolve",
    "bundle",
    "size"
  ],
  "peerDependencies": {
    "rollup": "^1.20.0||^2.0.0||^3.0.0"
  },
  "peerDependenciesMeta": {
    "rollup": {
      "optional": true
    }
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^7.2.0",
    "gzip-size": "^6.0.0",
    "lodash": "^4.17.21",
    "opener": "^1.5.2",
    "sirv": "^1.0.7",
    "terser": "^5.15.1",
    "ws": "^7.3.1"
  },
  "types": "./types/index.d.ts",
  "ava": {
    "files": [
      "!**/fixtures/**",
      "!**/output/**",
      "!**/helpers/**",
      "!**/recipes/**",
      "!**/types.ts"
    ]
  }
}
